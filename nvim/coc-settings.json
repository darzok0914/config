{
  "list.insertMappings": {
    "<C-n>": "normal:t",
    "<C-p>": "normal:s"
  },
  "suggest.enablePreview": false,
  "suggest.echodocSupport": true,
  "suggest.timeout":500,
  "suggest.completionItemKindLabels": {
    "keyword": "\uf1de",
    "variable": "\ue79b",
    "value": "\uf89f",
    "operator": "\u03a8",
    "function": "\u0192",
    "reference": "\ufa46",
    "constant": "\uf8fe",
    "method": "\uf09a",
    "struct": "\ufb44",
    "class": "\uf0e8",
    "interface": "\uf417",
    "text": "\ue612",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "module": "\uf40d",
    "color": "\ue22b",
    "property": "\ue624",
    "field": "\uf9be",
    "unit": "\uf475",
    "event": "\ufacd",
    "file": "\uf723",
    "folder": "\uf114",
    "snippet": "\ue60b",
    "typeParameter": "\uf728",
    "default": "\uf29c"
  },
  "diagnostic.enable": true,
  "diagnostic.enableSign": true,
  "diagnostic.highlightOffset": 100,
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextCurrentLineOnly": false,
  "diagnostic.virtualTextPrefix": "← ",
  "diagnostic.errorSign": "",
  "diagnostic.infoSign": "",
  "diagnostic.warningSign": "",
  "diagnostic.hintSign": "",
  "diagnostic.refreshOnInsertMode": true,
  "diagnostic.refreshAfterSave": true,
  "codeLens.enable": true,
  "list.selectedSignText": "·",
  "list.nextKeymap": "<C-t>",
  "list.previousKeymap": "<C-s>",
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "coc.preferences.formatOnInsertLeave": true,
  "coc.preferences.formatOnSaveFiletypes": ["rust"],
  "explorer.icon.enableNerdfont": true,
  "explorer.icon.enableVimDevicons": true,
  "explorer.previewAction.onHover": false,
  "explorer.keyMappings": {
    "i": false, // cancel default mapkey

    "s": "nodePrev",
    "t": "nodeNext",
    "gs": "expandablePrev",
    "gt": "expandableNext",

    "*": "toggleSelection",
    "<tab>": "actionMenu",

    "h": "collapse",
    "l": ["expandable?", "expand", "open"],
    "J": ["toggleSelection", "nodeNext"],
    "K": ["toggleSelection", "nodePrev"],
    "gl": "expandRecursive",
    "gh": "collapseRecursive",
    "<cr>": ["expandable?", ["expanded?", "collapse", "expand"], "open"],
    "e": "open",
    "o": "open:split",
    "O": "open:split:plain",
    "v": "open:vsplit",
    "T": "open:tab",
    "<bs>": "gotoParent",
    "gp": "preview:labeling",

    "y": "copyFilepath",
    "Y": "copyFilename",
    "à": "copyFile",
    "C": "cutFile",
    "p": "pasteFile",
    "d": "delete",
    "D": "deleteForever",

    "a": "addFile",
    "A": "addDirectory",

    ".": "toggleHidden",
    "R": "refresh",

    "?": "help",
    "q": "quit",
    "<esc>": "esc",
    "X": "systemExecute",
    "gd": "listDrive",

    "f": "search",
    "F": "searchRecursive",

    "gf": "gotoSource:file",
    "gb": "gotoSource:buffer",

    "[[": "sourcePrev",
    "]]": "sourceNext",

    "[d": "diagnosticPrev",
    "]d": "diagnosticNext",

    "[C": "gitPrev",
    "]C": "gitNext",
    "<<": "gitStage",
    ">>": "gitUnstage"
  },
  "rust-analyzer.inlayHints.chainingHints": true,
  "rust-analyzer.inlayHints.refreshOnInsertMode": true,
  "coc.preferences.formatOnType": true,
  "java.autobuild.enabled": false,
  "java.maven.downloadSources": true,
  "java.configuration.updateBuildConfiguration": "interactive",
  "java.referencesCodeLens.enabled": true,
  "java.implementationsCodeLens.enabled": true,
  "java.format.enabled": true,
  "bookmark.sign": "",
  "session.directory": "~/.local/share/nvim/session"
}
